#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Work/iVALT/C2PA/ivalt-api-hook/node_modules/loose-envify/node_modules:/Volumes/Work/iVALT/C2PA/ivalt-api-hook/node_modules:/Volumes/Work/iVALT/C2PA/node_modules:/Volumes/Work/iVALT/node_modules:/Volumes/Work/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Work/iVALT/C2PA/ivalt-api-hook/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Work/iVALT/C2PA/ivalt-api-hook/node_modules/loose-envify/node_modules:/Volumes/Work/iVALT/C2PA/ivalt-api-hook/node_modules:/Volumes/Work/iVALT/C2PA/node_modules:/Volumes/Work/iVALT/node_modules:/Volumes/Work/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Work/iVALT/C2PA/ivalt-api-hook/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../loose-envify/cli.js" "$@"
else
  exec node  "$basedir/../loose-envify/cli.js" "$@"
fi
